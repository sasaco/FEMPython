{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "make",
            "type": "shell",
            "command": "make",
            "options": {
                "cwd": "${workspaceFolder}/core"
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "somove",
            "type": "shell",
            "command": "mv ${workspaceFolder}/core/core.cpython-310-x86_64-linux-gnu.so ${workspaceFolder}/src/core.so",
            "dependsOn": [
                "make"
            ]
        }
    ]
}